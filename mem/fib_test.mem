// 1. Setup
00a00093 // addi x1, x0, 10  (Limit = 10)
00100113 // addi x2, x0, 1   (Current = 1)
00000193 // addi x3, x0, 0   (Prev = 0)
00100213 // addi x4, x0, 1   (Counter = 1)

// 2. Loop Label (Address 0x10)
00120C63 // beq x4, x1, +24  (Jump to Done if Counter == Limit)
003102B3 // add x5, x2, x3   (Temp = Curr + Prev)
002001B3 // add x3, x0, x2   (Prev = Curr)
005001B3 // add x2, x0, x5   (Curr = Temp)
00120213 // addi x4, x4, 1   (Counter++)
FEDFF06F // jal x0, -20      (Jump back to 0x10)

// 3. Done Label
00202023 // sw x2, 0(x0)      (Store Result)
00000013 // nop
00000013 // nop
00000013 // nop